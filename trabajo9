historial5 = (8520, 9510, 7530, 3570, 1590)

def valorMaximo( historial5):

    mayor=historial5[0]

    for i in range(1,len(historial5)):
        if historial5[i]>mayor:
            mayor=historial5[i]

        return print("el valor maximo gastado consultas a Toto es de $ ", mayor)

valorMaximo( historial5)
